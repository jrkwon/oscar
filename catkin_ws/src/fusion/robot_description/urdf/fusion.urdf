<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="fusion">

  <!-- #################################################################### -->
  <link name="base_link">
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
  </link>
  <joint name="base_link_connection" type="fixed">
    <parent link="base_link"/>
    <child link="chassis"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
  </joint>

  <!-- #################################################################### -->
  <link name="chassis">
    <visual>
      <origin xyz="0 0 0.03" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://fusion/robot_description/meshes/fusion/body.dae" scale="1.3 1.3 1.3"/>
      </geometry>
    </visual>
    <collision name="chassis">
      <origin xyz="0 -0.0550 0.8" rpy="0 0 0"/>
      <geometry>
        <box size="2.4 2.7 0.9"/>
      </geometry>
    </collision>
    <collision name="front_bumper">
      <origin xyz="0 -2.9 0.55" rpy="0.0 0 0"/>
      <geometry>
        <box size="2.3 0.45 0.65"/>
      </geometry>
    </collision>
    <collision name="hood">
      <origin xyz="0 -2.24 1.19" rpy="0.191247 -0 0"/>
      <geometry>
        <box size="2.1 1.41 0.075"/>
      </geometry>
    </collision>
    <collision name="windshield">
      <origin xyz="0 -1.1 1.57" rpy="0.43 0 0"/>
      <geometry>
        <box size="2.0 1.0 0.05"/>
      </geometry>
    </collision>
    <collision name="top_front">
      <origin xyz="0 -0.11 1.86" rpy="0.135030 0 0"/>
      <geometry>
        <box size="1.74 1.0 0.05"/>
      </geometry>
    </collision>
    <collision name="top_rear">
      <origin xyz="0 0.93 1.86" rpy="-0.119 0 0"/>
      <geometry>
        <box size="1.74 1.11 0.05"/>
      </geometry>
    </collision>
    <collision name="rear_window">
      <origin xyz="0 1.97 1.63" rpy="-0.262017 0 0"/>
      <geometry>
        <box size="1.74 0.896 0.05"/>
      </geometry>
    </collision>
    <collision name="trunk">
      <origin xyz="0 2.57 1.24" rpy="0.0 0 0"/>
      <geometry>
        <box size="1.78806 0.9 0.38"/>
      </geometry>
    </collision>
    <collision name="back_bumper">
      <origin xyz="0 2.7 0.7" rpy="0.0 0 0"/>
      <geometry>
        <box size="2.3 0.7 0.6"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1356.0"/>
      <origin xyz="0 0 1.0" rpy="0 0 1.57"/>
      <!-- inertia ixx="2581.13354740" ixy="0.0" ixz="0.0" iyy="591.30846112" iyz="0.0" izz="2681.95008628"/ -->
      <!-- inertia ixx="1000.0" ixy="0" ixz="0" iyy="1000.0" iyz="0" izz="1000.0"/-->
      <inertia ixx="742.0" ixy="0" ixz="0" iyy="4386.94" iyz="0" izz="3827.59"/>
    </inertial>
  </link>

  <!-- #################################################################### -->
  <!-- front axles -->
  <link name="fl_axle">
    <inertial>
      <mass value="1"/>
      <!-- origin xyz="0 0.45 0"/ -->
      <origin xyz="0 0 0"/>
      <inertia ixx="0.004" ixy="0.0" ixz="0.0" iyy="0.004" iyz="0.0" izz="0.004"/>
    </inertial>
  </link>
  <link name="fr_axle">
    <inertial>
      <mass value="1"/>
      <!-- origin xyz="0 0.45 0"/ -->
      <origin xyz="0 0 0"/>
      <inertia ixx="0.004" ixy="0.0" ixz="0.0" iyy="0.004" iyz="0.0" izz="0.004"/>
    </inertial>
  </link>

  <!-- #################################################################### -->
  <!-- front wheels -->
  <link name="front_left_wheel">
    <inertial>
      <mass value="11"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.58631238" ixy="0.0" ixz="0.0" iyy="0.33552910" iyz="0.0" izz="0.33552910"/>
    </inertial>
    <collision name="front_left_wheel_collision">
      <geometry>
        <sphere radius="0.47"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://fusion/robot_description/meshes/fusion/wheel.dae" scale="0.014 0.014 0.014"/>
      </geometry>
    </visual>
  </link>
  <link name="front_right_wheel">
    <inertial>
      <mass value="11"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.58631238" ixy="0.0" ixz="0.0" iyy="0.33552910" iyz="0.0" izz="0.33552910"/>
    </inertial>
    <collision name="front_right_wheel_collision">
      <geometry>
        <sphere radius="0.47"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 3.14"/>
      <geometry>
        <mesh filename="package://fusion/robot_description/meshes/fusion/wheel.dae" scale="0.014 0.014 0.014"/>
      </geometry>
    </visual>
  </link>

  <!-- #################################################################### -->
  <!-- rear wheels -->
  <link name="rear_left_wheel">
    <inertial>
      <mass value="11"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.58631238" ixy="0.0" ixz="0.0" iyy="0.33552910" iyz="0.0" izz="0.33552910"/>
    </inertial>
    <collision name="rear_left_wheel_collision">
      <geometry>
        <sphere radius="0.47"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://fusion/robot_description/meshes/fusion/wheel.dae" scale="0.014 0.014 0.014"/>
      </geometry>
    </visual>
  </link>
  <link name="rear_right_wheel">
    <inertial>
      <mass value="11"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.58631238" ixy="0.0" ixz="0.0" iyy="0.33552910" iyz="0.0" izz="0.33552910"/>
    </inertial>
    <collision name="rear_right_wheel_collision">
      <geometry>
        <sphere radius="0.47"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 3.14"/>
      <geometry>
        <mesh filename="package://fusion/robot_description/meshes/fusion/wheel.dae" scale="0.014 0.014 0.014"/>
      </geometry>
    </visual>
  </link>

  <!-- #################################################################### -->
  <!-- sterring wheel -->
  <link name="steering_wheel">
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.14583300" ixy="0.0" ixz="0.0" iyy="0.14583300" iyz="0.0" izz="0.125"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.302101 0 0"/>
      <geometry>
        <mesh filename="package://fusion/robot_description/meshes/fusion/steering_wheel.obj" scale="0.013 0.013 0.013"/>
      </geometry>
    </visual>
  </link>

  <!-- #################################################################### -->
  <!-- joints for steering and wheel joints -->
  <joint name="steering_joint" type="continuous">
    <origin xyz="0.5439212 -0.8 1.3" rpy="-1.302101 0 0"/>
    <parent link="chassis"/>
    <child link="steering_wheel"/>
    <axis xyz="0 0 1"/>
    <limit lower="-7.85" upper="7.85" effort="10000000" velocity="1000000"/>
  </joint>
  <joint name="front_left_steer_joint" type="continuous">
    <parent link="chassis"/>
    <child link="fl_axle"/>
    <origin xyz="1.1 -2.0 0.54" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.8727" upper="0.8727" effort="10000000" velocity="1000000"/>
  </joint>
  <joint name="front_right_steer_joint" type="continuous">
    <parent link="chassis"/>
    <child link="fr_axle"/> 
    <origin xyz="-1.1 -2.0 0.54" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.8727" upper="0.8727" effort="10000000" velocity="1000000"/>
  </joint>
  <joint name="front_left_wheel_joint" type="continuous">
    <parent link="fl_axle"/>
    <child link="front_left_wheel"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
  </joint>
  <joint name="front_right_wheel_joint" type="continuous">
    <parent link="fr_axle"/>
    <child link="front_right_wheel"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
  </joint>
  <joint name="rear_left_wheel_joint" type="continuous">
    <parent link="chassis"/>
    <child link="rear_left_wheel"/>
    <origin xyz="1.1 1.8 0.54" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
  </joint>
  <joint name="rear_right_wheel_joint" type="continuous">
    <parent link="chassis"/>
    <child link="rear_right_wheel"/>
    <origin xyz="-1.1 1.8 0.54" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
  </joint>

  <!-- #################################################################### -->
  <!-- Sensor links and fixed joints -->

  <link name="back_camera_link">
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
  </link>
  <joint name="back_camera_joint" type="fixed">
    <parent link="chassis"/>
    <child link="back_camera_link"/>
    <origin xyz="0 1.65 1.82" rpy="0 0.05 1.5707"/>
  </joint>
  <link name="left_camera_link">
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
  </link>
  <joint name="left_camera_joint" type="fixed">
    <parent link="chassis"/>
    <child link="left_camera_link"/>
    <!-- origin xyz="0.8 -0.7 2.52" rpy="0 0.05 -1.94"/ -->
    <origin xyz="0.8 -0.6 1.83" rpy="0 0.05 -1.5707"/>
  </joint>
  <link name="front_camera_link">
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
  </link>
  <joint name="front_camera_joint" type="fixed">
    <parent link="chassis"/>
    <child link="front_camera_link"/>
    <!-- origin xyz="0 -0.7 2.52" rpy="0 0.05 -1.5707"/ -->
    <origin xyz="0 -0.6 1.83" rpy="0 0.05 -1.5707"/>
  </joint>
  <link name="right_camera_link">
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
  </link>
  <joint name="right_camera_joint" type="fixed">
    <parent link="chassis"/>
    <child link="right_camera_link"/>
    <!-- origin xyz="-0.8 -0.7 2.52" rpy="0 0.05 -1.20"/ -->
    <origin xyz="-0.8 -0.6 1.83" rpy="0 0.05 -1.5707"/>
  </joint>

  <link name="imu_link">
    <inertial>
      <mass value="1" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0"
               iyy="0.001" iyz="0.0" 
               izz="0.001" />
    </inertial>     
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <box size = "0.1 0.1 0.1"/> 
        </geometry>
      </visual>
    <collision>         
        <geometry>
            <box size = "0.1 0.1 0.1"/> 
        </geometry>
    </collision>     
  </link>
  <joint name="imu_joint" type="fixed">
    <axis xyz="0 0 0"/>
    <origin xyz="0.05 0 0.15" rpy="0 0 0" />
    <parent link="chassis"/>
    <child link="imu_link" />
  </joint>

  <link name="gps_link">
    <inertial>
      <mass value="1" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0"
               iyy="0.001" iyz="0.0" 
               izz="0.001" />
    </inertial>     
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
          <box size = "0.1 0.1 0.1"/> 
      </geometry>
    </visual>
    <collision>         
        <geometry>
            <box size = "0.1 0.1 0.1"/> 
        </geometry>
    </collision>     
  </link>
  <joint name="gps_joint" type="fixed">
    <axis xyz="0 0 0"/>
    <origin xyz="0.05 0 0.15" rpy="0 0 0" />
    <parent link="chassis"/>
    <child link="gps_link" />
  </joint>

  <link name="velodyne_link">
    <inertial>
        <mass value="1" />
        <origin xyz="0 0 0" />
        <inertia ixx="0.001" ixy="0.0" ixz="0.0"
                 iyy="0.001" iyz="0.0" 
                 izz="0.001" />
    </inertial>     
    <visual>
        <origin xyz="0 0 -0.0377" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://fusion/models/velodyne_VLP16/meshes/VLP16_base_1.dae" />
        </geometry>
    </visual>
    <visual>
        <origin xyz="0 0 -0.0377" />
        <geometry>
          <mesh filename="package://fusion/models/velodyne_VLP16/meshes/VLP16_base_2.dae" />
        </geometry>
    </visual>
    <visual>
        <origin xyz="0 0 -0.0377" />
        <geometry>
          <mesh filename="package://fusion/models/velodyne_VLP16/meshes/VLP16_scan.dae" />
        </geometry>
    </visual>
    <collision>
        <origin rpy="0 0 0" xyz="0 0 0.03585"/>
        <geometry>
          <cylinder radius="0.0516" length="0.0717"/>
        </geometry>
    </collision>            
  </link>
  <joint name="velodyne_joint" type="fixed">
    <axis xyz="0 0 0"/>
    <!-- origin xyz="0 -0.7 2.52" rpy="0 0 -1.57"/ -->
    <origin xyz="0 -0.5 1.86" rpy="0 0 -1.57"/>
    <parent link="chassis"/>
    <child link="velodyne_link" />
  </joint> 
